<%= form_with model: @spot, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control', id: 'spot-name' %>
  <%= f.label :address %>
  <%= f.text_field :address, class: 'form-control', rows: 10 %>
  <%= f.label :prefecture %>
  <%= f.text_field :prefecture, class: 'form-control', rows: 10 %>
  <%= f.label :place_id %>
  <%= f.text_field :place_id, class: 'form-control', id: 'spot-name' %>
  <%= f.label :latitude %>
  <%= f.text_field :latitude, class: 'form-control', id: 'spot-name' %>
  <%= f.label :longitude %>
  <%= f.text_field :longitude, class: 'form-control', id: 'spot-name' %>

  <%= f.submit class: 'btn btn-primary' %>
<% end %>
